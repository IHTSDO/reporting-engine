<?xml version="1.0" encoding="UTF-8"?>
<beans 
	xmlns="http://www.springframework.org/schema/beans"
	xmlns:mvc="http://www.springframework.org/schema/mvc"
	xmlns:context="http://www.springframework.org/schema/context"

	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans-3.1.xsd
	http://www.springframework.org/schema/context http://www.springframework.org/schema/context/spring-context-3.1.xsd
    http://www.springframework.org/schema/mvc http://www.springframework.org/schema/mvc/spring-mvc-3.1.xsd">

	<mvc:default-servlet-handler />

	<context:property-placeholder
			location="service_configuration.properties"
			ignore-resource-not-found="true" ignore-unresolvable="true" order="0" />

	<context:component-scan base-package="org.ihtsdo.snowowl.authoring.batchimport.api.rest" />

	<import resource="services-context.xml"/>

</beans>
